@model YSD.Web.Models.FeaturedUsersViewModel
<h2>Featured Products</h2>
<div>
    <table style="width:70%;">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
        @foreach (var user in this.Model.Users)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
            </tr>
        }
    </table>
</div>